<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="ir_seq_material_request" model="ir.sequence">
            <field name="name">Material Request Sequence</field>
            <field name="code">maintenance.material.request</field>
            <field name="prefix">Req/%(range_year)s/</field>
            <field name="padding">5</field>
        </record>
        <record id="ir_seq_return_material_request" model="ir.sequence">
            <field name="name">Return Material Request Sequence</field>
            <field name="code">maintenance.material.request.return</field>
            <field name="prefix">Ret/%(range_year)s/</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>