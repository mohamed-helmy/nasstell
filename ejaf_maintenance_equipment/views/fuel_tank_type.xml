                <tree string="Tower type">

                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="fuel_tank_type_action_view" model="ir.actions.act_window">
            <field name="name">Fuel Tank Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">fuel.tank.type</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                </p>
