<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="rect_brand_form_view" model="ir.ui.view">
            <field name="name">rect.brand.form</field>
            <field name="model">rect.brand</field>
            <field name="arch" type="xml">
                <form string="Rect Brand">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="rect_brand_tree_view" model="ir.ui.view">
            <field name="name">rect.brand.tree</field>
            <field name="model">rect.brand</field>
            <field name="arch" type="xml">
                <tree string="Rect Brand">

                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="rect_brand_action_view" model="ir.actions.act_window">
            <field name="name">Rect Brand</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">rect.brand</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                </p>
                <p>
                    <!-- More details about what a user can do with this object will be OK -->
                </p>
            </field>
        </record>
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="rect_brand_action_menu" name="Rect Brand"
                  parent="maintenance.menu_maintenance_configuration" action="rect_brand_action_view"
                  sequence="13"/>


    </data>
</odoo>