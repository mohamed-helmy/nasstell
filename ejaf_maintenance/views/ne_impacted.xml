<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ne_impacted_form_view" model="ir.ui.view">
            <field name="name">ne.impacted.form</field>
            <field name="model">ne.impacted</field>
            <field name="arch" type="xml">
                <form string="Ne Impacted">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="ne_impacted_tree_view" model="ir.ui.view">
            <field name="name">ne.impacted.tree</field>
            <field name="model">ne.impacted</field>
            <field name="arch" type="xml">
                <tree string="Ne Impacted">

                    <field name="name"/>

                </tree>
            </field>
        </record>
        <record id="ne_impacted_action_view" model="ir.actions.act_window">
            <field name="name">Ne Impacted</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ne.impacted</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>