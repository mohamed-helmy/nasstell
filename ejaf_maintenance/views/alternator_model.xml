<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="alternator_model_form_view" model="ir.ui.view">
            <field name="name">alternator.model.form</field>
            <field name="model">alternator.model</field>
            <field name="arch" type="xml">
                <form string="Alternator Model">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="alternator_model_tree_view" model="ir.ui.view">
            <field name="name">alternator.model.tree</field>
            <field name="model">alternator.model</field>
            <field name="arch" type="xml">
                <tree string="Alternator Model">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="alternator_model_action_view" model="ir.actions.act_window">
            <field name="name">Alternator Model</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">alternator.model</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>