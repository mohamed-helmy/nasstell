<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ne_type_form_view" model="ir.ui.view">
            <field name="name">ne.type.form</field>
            <field name="model">ne.type</field>
            <field name="arch" type="xml">
                <form string="Ne Type">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="ne_type_tree_view" model="ir.ui.view">
            <field name="name">ne.type.tree</field>
            <field name="model">ne.type</field>
            <field name="arch" type="xml">
                <tree string="Ne Type">

                    <field name="name"/>

                </tree>
            </field>
        </record>
        <record id="ne_type_action_view" model="ir.actions.act_window">
            <field name="name">Ne Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ne.type</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>